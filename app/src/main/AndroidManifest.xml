<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="se.grupp1.antonsskafferi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activites.SplashscreenActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.SEND" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activites.LoginActivity" />
        <activity android:name=".fragments.ScheduleFragment" />
        <activity android:name=".fragments.BookingFragment" />
        <activity android:name=".activites.MainActivity" />
        <activity android:name=".fragments.SettingsFragment" />
        <activity android:name=".fragments.EditLunchFragment" />
    </application>

</manifest>